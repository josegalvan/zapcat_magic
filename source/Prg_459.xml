        <SQL_FORM DB="ZapCat" RESTAB="Memory">
          <SQL_STMT_U val="select p.razonsocial,a.idmarca,a.pagina,a.idestilo,a.idcolor,a.talla,a.precio from pedidoslines l inner join pedidosheader h on ( l.empresaNo=h.EmpresaNo and l.pedido=h.pedidoNo ) inner join pedidos_status_fechas f on (f.empresaNo=l.empresano and l.pedido=f.pedido and l.productoNo=f.productono and l.catalogo=f.catalogo and f.nolinea=l.nolinea and l.status=f.status) inner join articulo a on (a.empresano=l.empresano and a.codigoarticulo=l.productono and a.catalogo=l.catalogo) inner join proveedor p on (a.idproveedor=p.proveedorno) where l.status='Aqui'  and  h.asociadono=1797 and h.idsucursal=1;"/>
