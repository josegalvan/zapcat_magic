        <SQL_FORM DB="ZapCat" RESTAB="ZapCat">
          <SQL_STMT_U val="select l.pedido, h.fechaultimamodificacion,a.idmarca,a.idestilo,a.idcolor,a.talla,l.precio,l.status from pedidoslines as l inner join pedidosheader as h  on (l.empresano=h.empresano and l.pedido=h.pedidono ) inner join articulo a on (l.empresano=a.empresano and l.productono=a.codigoarticulo and a.catalogo=l.catalogo) where h.asociadono=':1' and l.status='Por Confirmar' and h.fechaultimamodificacion>=':2' and h.fechaultimamodificacion&lt;=':3';"/>
